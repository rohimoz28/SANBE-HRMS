<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
    
        <record id="sb_view_overtime_review_view_tree" model="ir.ui.view">
            <field name="name">sb.view.overtime.review.view.tree</field>
            <field name="model">sb.view.overtime.review</field>
            <field name="arch" type="xml">
                <tree string="Overtime Review">
                    <field name="area_id" />
                    <field name="name" />
                    <field name="request_date" />
                    <field name="state" />
                    <field name="department_id" />
                    <field name="branch_id" />
                    <field name="periode_from" />
                    <field name="periode_to" />
                </tree>
            </field>
        </record>

        <record id="sb_view_overtime_review_view_search" model="ir.ui.view">
            <field name="name">sb.view.overtime.review.view.search</field>
            <field name="model">sb.view.overtime.review</field>
            <field name="arch" type="xml">
                <search string="Overtime Review">
                    <field name="area_id" />
                    <field name="name" />
                    <field name="request_date" />
                    <field name="state" />
                    <field name="department_id" />
                    <field name="branch_id" />
                    <field name="periode_from" />
                    <field name="periode_to" />
                </search>
            </field>
        </record>

        <record id="sb_view_overtime_review_action" model="ir.actions.act_window">
            <field name="name">Overtime Review</field>
            <field name="res_model">sb.view.overtime.review</field>
            <field name="view_mode">tree</field>
            <field name="domain">[]</field>
            <field name="context">{}</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No Report Overtime Review
                </p>
            </field>
        </record>
    
    </data>

</odoo>
