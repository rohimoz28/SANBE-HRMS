<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inherit_sb_incomplete_attendance_details_form_view" model="ir.ui.view">
            <field name="name">inherit.sb.incomplete.attendance.details.form.view</field>
            <field name="model">sb.incomplete.attendances</field>
            <field name="inherit_id" ref="sanbe_hr_tms.sb_incomplete_attendances_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='sb_incomplete_attendance_details_id']" position="inside">
                    <field name="incomplete_attn_detail_ids">
                        <tree editable="bottom" create="0" delete="0" edit="0">
                            <field name="period_id" string="Period"/>
                            <field name="area_id" string="Area"/>
                            <field name="branch_id" string="Branch"/>
                            <field name="nik" string="NIK"/>
                            <field name="name" string="Name"/>
                            <field name="department_id" string="Department"/>
                            <field name="job_id" string="Job Position"/>
                            <field name="wdcode" string="WD Code"/>
                            <field name="empgroup_id" string="Emp Group"/>
                            <field name="details_date" string="Date"/>
                            <field name="date_timein" string="Tgl Time in"/>
                            <field name="time_in" widget="float_time"/>
                            <field name="date_timeout" string="Tgl Time out"/>
                            <field name="time_out" widget="float_time"/>
                            <field name="status_attendance" string="Status"/>
                        </tree>
                    </field>
                </xpath>
            </field>
        </record>

    </data>
</odoo>