                        <group>
                            <group>
                                <field name="workingday_id"/>
                                <field name="type" readonly="1"/>
                                <field name="date_in"/>
                                <field name="date_out"/>
                                <field name="status_attendee" readonly="1"/>
                                <field name="delay" readonly="1"/>
                                <field name="status" readonly="1"/>
                                <field name="emp_id" readonly="1"/>
                                <field name="approval_ot_from" readonly="1" widget="float_time"/>
                                <field name="approval_ot_to" readonly="1" widget="float_time"/>
                                <field name="edited_time_in" widget="float_time"/>
                                <field name="edited_time_out" widget="float_time"/>
