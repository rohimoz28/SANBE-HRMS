    <data>

        <record id="tmsentry_details_form_view" model="ir.ui.view">
            <field name="name">sanbe_hr_tms.sb.tms.tmsentry.details.form</field>
            <field name="model">sb.tms.tmsentry.details</field>
            <field name="arch" type="xml">
                <form string="TMS Entry Details">
                    <sheet>
                        <!-- <div class="oe_title"> -->
                        <!--     <label for="details_date"/> -->
                        <!--     <h1> -->
                        <!--         <field name="details_date" placeholder="Details Date"/> -->
                        <!--     </h1> -->
