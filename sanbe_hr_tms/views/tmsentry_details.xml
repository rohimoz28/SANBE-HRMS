                            </group>
                            <group>
                                <field name="details_date" readonly="1"/>
                                <field name="time_in" widget="easy_timepicker" readonly="1"/>
                                <field name="time_out" widget="easy_timepicker" readonly="1"/>
                                <field name="status_attendance" readonly="1"/>
                                <field name="delay" string="Delay" readonly="1"/>
                                <field name="empgroup_id" readonly="1"/>
                                <field name="aot1" readonly="1"/>
                                <field name="aot2" readonly="1"/>
                                <field name="aot3" readonly="1"/>
                                <field name="aot4" readonly="1"/>
                                <field name="oto" readonly="1"/>
                                <field name="approved"/>
                                <field name="approved_by_ca"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

