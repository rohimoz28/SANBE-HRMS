<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="sanbe_hr_tms.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <button t-if="props.resModel == 'sb.overtime.bundling'" class="btn btn-primary" data-hotkey="r" t-on-click="showOTBundling">
                Export to Excel
            </button>
        </xpath>
    </t>
</templates>