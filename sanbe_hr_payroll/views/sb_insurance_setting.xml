        <!--     </field> -->
        <!-- </record> -->

        <record id="action_payroll_insurance_setting" model="ir.actions.act_window">
            <field name="name">Insurance Setting</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">sb.insurance.setting</field>
            <!-- <field name="view_type">form</field> -->
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new Insurance Setting.
                </p>
            </field>
        </record>

        <!-- <menuitem name="ModelTitle" id="ModelName_menu" parent="your_root_menu" action="ModelName_act_window"/> -->

